  ],
  "connections": {
    "Start": {
      "main": [
        [
          {
            "node": "Discord Self-Bot - Send Message",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Discord Self-Bot - Send Message": {
      "main": [
        [
          {
            "node": "Discord Self-Bot - Read Messages",
            "type": "main",
            "index": 0
          }
        ]
      ]
