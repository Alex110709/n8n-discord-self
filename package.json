{
  "name": "n8n-nodes-discord-self",
  "version": "2.0.0",
  "description": "n8n node for Discord self-bot - Full Discord API functionality",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "discord",
    "self-bot"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Alex110709/n8n-discord-self",
  "author": {
    "name": "Alex110709",
    "email": "alex110709@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alex110709/n8n-discord-self.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-images",
    "copy-images": "copyfiles -u 1 \"src/**/*.{png,svg}\" dist/",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DiscordSelfApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/DiscordSelf/DiscordSelf.node.js",
      "dist/nodes/DiscordSelf/DiscordSelfV2.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.19.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "discord.js-selfbot-v13": "^2.14.13",
    "n8n-core": "^1.19.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.10",
    "npm": ">=8.0.0"
  }
}
